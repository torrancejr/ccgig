

<h3>Reviews for <%= @user.username %></h3>
<div class="row">
  <div class="small-10 columns small-centered">
    <%= link_to "Add a Review",  new_user_review_path(@user), class: "button"%>
  <% @reviews.each do |review| %>
  <div class="card">

    <div class="card-info">
    <h3 class="card-title">Rating: <%= review.rating %><br /></h3>
    <p class="card-author">Comments: <%= review.body %> </p>
    </div>
  </div>
    <% end %>
  </div>
</div>
